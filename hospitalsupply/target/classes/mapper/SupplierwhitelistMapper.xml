<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cuit.hospitalsupply.mapper.SupplierwhitelistMapper">

    <resultMap id="BaseResultMap" type="com.cuit.hospitalsupply.pojo.Supplierwhitelist">
            <id property="supplierid" column="supplierid" jdbcType="VARCHAR"/>
            <id property="whitelistid" column="whitelistid" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        supplierid,whitelistid
    </sql>
    <delete id="remove" parameterType="com.cuit.hospitalsupply.pojo.Supplierwhitelist">
        delete from t_supplierwhitelist where supplierid=#{supplierwhitelist.supplierid} and whitelistid=#{supplierwhitelist.whitelistid}
    </delete>
</mapper>
